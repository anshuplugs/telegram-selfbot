# .env template
# Replace with your values

# If left blank, the script will prompt for the API ID / Hash / Phone on first run
API_ID=
API_HASH=
# Optionally add PHONE_NUMBER for first-run convenience (will still prompt if missing)
PHONE_NUMBER=

# Prefixes separated by space; defaults: . # / !
PREFIXES=. # / !

# If you want ads/bulk enabled set to "true" (case-insensitive)
ADS_ENABLED=false

# If you want the config (api_id/api_hash/phone/2fa) encrypted, set a base64 Fernet key here.
# You can generate one with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
# If left empty and cryptography available, the script will generate one and print it â€” store it somewhere safe.
ENCRYPT_CONFIG_KEY=

# Optional OpenAI key for .ai
OPENAI_API_KEY=

# Session file name (optional)
SESSION_NAME=anshu
